{
  "extends": "airbnb-base/legacy",
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "prefer-const": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "padded-blocks": 0,
    "space-before-function-paren": 0,
    "no-nested-ternary": 0,
    "no-undef": 1,
    "no-unused-vars": 1,
    "max-len": 0,
    "func-names": 0
  }
}
